{
    "name": "Koinex Moving Averages",
    "description" : "Moving Averages for Koinex",
    "version": "1.0.2",
    "manifest_version": 2,

    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },

    "content_scripts": [
        {
          "matches": [
            "https://*.koinex.in/",
            "https://*.koinex.in/exchange/*"
          ],
          "js": ["jquery-3.2.1.min.js", "content.js"]
        }
    ],
    "web_accessible_resources": ["script.js"]
}
